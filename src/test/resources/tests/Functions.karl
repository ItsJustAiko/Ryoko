use std::io;

declare func sayHello::(string: name): void -> {
    Show("Hello, ", name, "!");
}

declare func main::(): void -> {
    Show("What is your name?");
    sayHello("Karl");
}

declare func getStr::(): string -> {
    return "Hello, World!";
}
main();
Show(getStr());
Show(getInt(9) + 1);

declare func getInt::(int: num): int -> {
    return num + 1;
}
Show(getInt(6) + 1);

declare func getIfInt::(int: number): int -> {
  if (number == 14) -> {
    return 1;
  } else -> {
    return 0;
  }
}

Show(getIfInt(14));