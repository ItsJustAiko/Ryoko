use std::io;

declare func sayHello::(string: name): void -> {
    io:Show("Hello, ", name, "!");
}

declare func main::(): void -> {
    io:Show("What is your name?");
    sayHello("Karl");
}

declare func getStr::(): string -> {
    return "Hello, World!";
}
main();
io:Show(getStr());
io:Show(getInt(9) + 1);

declare func getInt::(int: num): int -> {
    return num + 1;
}
io:Show(getInt(6) + 1);

declare func getIfInt::(int: number): int -> {
  if (number == 14) -> {
    return 1;
  } else -> {
    return 0;
  }
}

io:Show(getIfInt(14));