use std::io;

// Make a lot of mathematical operations with the result
declare int: one = 5 + (3+3) * 2; // 17
declare int: two = 5 + 3 + 3 * 2; // 14
declare int: three = (5 + 3 + 3) * 2; // 22
declare int: four = 5 + 3 + (3 * 2); // 14
declare int: five = (8*9) / 2; // 36
declare int: six = 8 * 9 / 2; // 36
declare int: seven = 8 * (9 / 2); // 36
declare float: eight = 4 + 5 * 2 / 3; // 7.3333..
declare int: nine = (4 + 5) * 2 / 3; // 6
declare float: ten = 4 + 5 * (2 / 3); // 7.333
declare int: eleven = 5 **2; // 25
declare int: twelve = (5 ** 2) * 5; // 125

// Print the results
io:WriteLn(one);
io:WriteLn(two);
io:WriteLn(three);
io:WriteLn(four);
io:WriteLn(five);
io:WriteLn(six);
io:WriteLn(seven);
io:WriteLn(eight);
io:WriteLn(nine);
io:WriteLn(ten);
io:WriteLn(eleven);
