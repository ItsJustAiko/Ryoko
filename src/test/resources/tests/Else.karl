use std::io;

declare int: number = 10;
if (number == 11) -> {
    io:WriteLn("No");
} else if (number == 10) -> {
    io:WriteLn("Test #1 success");
}

if (number < 10) -> {
    io:WriteLn("No");
} else -> {
    io:WriteLn("Test #2 success");
}

declare func main::(): void -> {
    if (!true) -> {
        io:WriteLn("No");
    } else -> {
        io:WriteLn("Test #3 success");
    }
}

main();

declare bool: testPassed = false;
if (testPassed) -> {
    io:WriteLn("No");
} else -> {
     testPassed = true;
}

if (testPassed) -> {
    io:WriteLn("Test #4 success");
}