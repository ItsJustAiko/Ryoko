use std::io;

declare int: number = 10;
if (number == 11) -> {
    io:Show("No");
} else if (number == 10) -> {
    io:Show("Test #1 success");
}

if (number < 10) -> {
    io:Show("No");
} else -> {
    io:Show("Test #2 success");
}

declare func main::(): void -> {
    if (!true) -> {
        io:Show("No");
    } else -> {
        io:Show("Test #3 success");
    }
}

main();

declare bool: testPassed = false;
if (testPassed) -> {
    io:Show("No");
} else -> {
     testPassed = true;
}

if (testPassed) -> {
    io:Show("Test #4 success");
}